{
    "info": "Bot+intents+sentiment (v24_a; 09.04.19) Alice V5 Bot.  Alice2Alice + Toloka_Alice_only. Total 203780 dialogs",
    "chainer": {
      "in": ["last_utterances", "utterances_histories"],
      "pipe": [
        {
          "info": "0. Converting list[json string] to list[list[string]]",
          "class_name": "skills.ranking_chitchat_2stage.utils:JSON2StrList",
          "in": ["utterances_histories"],
          "out": ["utterances_histories"]
        },
        {
          "info": "1. Replace separators from input string",
          "class_name": "compose_inputs_hybrid_ranker",
          "context_depth": 3,
          "model_context_depth": 3,
          "use_user_context_only": true,
          "use_context_for_query": false,
          "in": ["last_utterances", "utterances_histories"],
          "out": ["queries_batch", "expanded_context_batch"]
        },
        {
          "class_name": "hashing_tfidf_vectorizer",
          "id": "vectorizer",
          "save_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/train_contexts_tfidf_matrix.npz",
          "load_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/train_contexts_tfidf_matrix.npz",
          "tokenizer":
          {
            "class_name": "ru_tokenizer",
            "alphas_only": false,
            "lemmas": true,
            "ngram_range": [1, 3]
          }
        },
        {
          "info": "2. Find relevant contexts",
          "class_name": "tfidf_ranker_adv",
          "max_n": [25, 425, 50],
          "in": ["queries_batch"],
          "out": ["tfidf_doc_ids", "tfidf_doc_scores"],
          "vectorizer": "#vectorizer"
        },
        {
          "info": "3. Retrieve response candidates using the context retrieved",
          "class_name": "retrieve_candidates",
          "map_filename": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/train_index_dict_v16.pickle",
          "in": ["expanded_context_batch", "tfidf_doc_ids", "tfidf_doc_scores"],
          "out": ["response_candidates", "model_inputs"]
        },
        {
          "info": "4. Preprocess response candidates and choose the best one",
          "id": "preproc",
          "class_name": "siamese_preprocessor",
          "load_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/preproc/tok.dict",
          "num_ranking_samples": 500,
          "num_context_turns": 10,
          "max_sequence_length": 50,
          "embedding_dim": 200,
          "in": ["model_inputs"],
          "out": ["x_proc"],
          "tokenizer": {
            "class_name": "ru_tokenizer",
            "id": "tok_1"
          },
          "vocab": {
            "class_name": "simple_vocab",
            "special_tokens": ["", "<UNK>"],
            "unk_token": "<UNK>",
            "id": "vocab_1",
            "load_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/vocabs/int_tok.dict",
            "save_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/vocabs/int_tok.dict"
          },
          "embedder": {
            "id": "word2vec_embedder",
            "class_name": "glove",
            "dim": 200,
            "load_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/chitchat1.w2v.vectors.txt"
          }
        },
        {
          "id": "embeddings",
          "class_name": "emb_mat_assembler",
          "embedder": "#word2vec_embedder",
          "vocab": "#vocab_1"
        },
        {
          "in": ["x_proc"],
          "out": ["y_predicted"],
          "class_name": "smn_nn",
          "use_logits": false,
          "num_context_turns": "#preproc.num_context_turns",
          "max_sequence_length": "#preproc.max_sequence_length",
          "embedding_dim": "#word2vec_embedder.dim",
          "emb_matrix": "#embeddings.emb_mat",
          "learning_rate": 1e-3,
          "batch_size": 500,
          "seed": 65,
          "load_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/model_smn/model",
          "save_path": "{MODELS_PATH}/chitchat_2staged_tfidf_smn_v16/model_smn/model"
        },
        {
          "info": "5. Return actual response",
          "class_name": "hybrid_ranker_predictor",
          "return_topn": true,
          "topn": 17,
          "in": ["response_candidates", "y_predicted"],
          "out": ["topn_responses", "topn_responses_scores"]
        },
        {
          "info": "6. Preprocess for sentiment classifier",
          "class_name": "preproc_sentiment",
          "in": ["expanded_context_batch", "topn_responses"],
          "out": ["preprocessed_input_sentiment"]
        },
        {
          "info": "7. Preprocess for intent classifier",
          "class_name": "preproc_intents",
          "in": ["expanded_context_batch", "topn_responses"],
          "out": "preprocessed_input_intents"
        },
        {
          "info": "8. Run sentiment classifier",
          "id": "classes_vocab_sentiment",
          "class_name": "simple_vocab",
          "save_path": "{SENTIMENT_MODELS_PATH}/classes.dict",
          "load_path": "{SENTIMENT_MODELS_PATH}/classes.dict"
        },
        {
          "in": "preprocessed_input_sentiment",
          "out": "x_prep_sentiment",
          "class_name": "dirty_comments_preprocessor"
        },
        {
          "in": "x_prep_sentiment",
          "out": "x_tok_sentiment",
          "id": "my_tokenizer_sentiment",
          "class_name": "nltk_tokenizer",
          "tokenizer": "wordpunct_tokenize"
        },
        {
          "in": "x_tok_sentiment",
          "out": "x_emb_sentiment",
          "id": "my_embedder_sentiment",
          "class_name": "fasttext",
          "load_path": "{DOWNLOADS_PATH}/embeddings/ft_native_300_ru_wiki_lenta_nltk_wordpunct_tokenize.bin",
          "pad_zero": true
        },
        {
          "in": "x_emb_sentiment",
          "out": "y_pred_probas_sentiment",
          "main": true,
          "class_name": "keras_classification_model",
          "save_path": "{SENTIMENT_MODELS_PATH}/model",
          "load_path": "{SENTIMENT_MODELS_PATH}/model",
          "embedding_size": "#my_embedder_sentiment.dim",
          "n_classes": "#classes_vocab_sentiment.len",
          "kernel_sizes_cnn": [
            3,
            5,
            7
          ],
          "filters_cnn": 256,
          "optimizer": "Adam",
          "learning_rate": 0.01,
          "learning_rate_decay": 0.1,
          "loss": "binary_crossentropy",
          "last_layer_activation": "softmax",
          "coef_reg_cnn": 1e-3,
          "coef_reg_den": 1e-2,
          "dropout_rate": 0.5,
          "dense_size": 100,
          "model_name": "cnn_model"
        },
        {
          "in": "y_pred_probas_sentiment",
          "out": "y_pred_ids_sentiment",
          "class_name": "proba2labels",
          "max_proba": true
        },
        {
          "in": "y_pred_ids_sentiment",
          "out": "y_pred_labels_sentiment",
          "ref": "classes_vocab_sentiment"
        },
        {
          "info": "9. Run intents classifier",
          "id": "classes_vocab_intents",
          "class_name": "simple_vocab",
          "save_path": "{INTENTS_MODELS_PATH}/classes.dict",
          "load_path": "{INTENTS_MODELS_PATH}/classes.dict"
        },
        {
          "in": "preprocessed_input_intents",
          "out": "x_prep_intents",
          "class_name": "dirty_comments_preprocessor"
        },
        {
          "in": "x_prep_intents",
          "out": "x_tok_intents",
          "id": "my_tokenizer_intents",
          "class_name": "nltk_tokenizer",
          "tokenizer": "wordpunct_tokenize"
        },
        {
          "in": "x_tok_intents",
          "out": "x_emb_intents",
          "id": "my_embedder_intents",
          "class_name": "fasttext",
          "load_path": "{DOWNLOADS_PATH}/embeddings/ft_native_300_ru_wiki_lenta_nltk_wordpunct_tokenize.bin",
          "pad_zero": true
        },
        {
          "in": ["x_emb_intents"],
          "out": ["y_pred_probas_intents"],
          "main": true,
          "class_name": "keras_classification_model",
          "save_path": "{INTENTS_MODELS_PATH}/model",
          "load_path": "{INTENTS_MODELS_PATH}/model",
          "embedding_size": "#my_embedder_intents.dim",
          "n_classes": "#classes_vocab_intents.len",
          "kernel_sizes_cnn": [
            3,
            5,
            7
          ],
          "filters_cnn": 256,
          "optimizer": "Adam",
          "learning_rate": 0.01,
          "learning_rate_decay": 0.1,
          "loss": "binary_crossentropy",
          "last_layer_activation": "softmax",
          "coef_reg_cnn": 1e-3,
          "coef_reg_den": 1e-2,
          "dropout_rate": 0.5,
          "dense_size": 100,
          "model_name": "cnn_model"
        },
        {
          "in": "y_pred_probas_intents",
          "out": "y_pred_ids_intents",
          "class_name": "proba2labels",
          "max_proba": true
        },
        {
          "in": "y_pred_ids_intents",
          "out": "y_pred_labels_intents",
          "ref": "classes_vocab_intents"
        },
        {
          "info": "10. Final reranker",
          "class_name": "chitchat_reranker",
          "lambda_coeff": 5.0,
          "in": ["topn_responses", "topn_responses_scores", "y_pred_labels_sentiment", "y_pred_labels_intents"],
          "out": ["best_responses", "response_preds"]
        }
      ],
      "out": ["best_responses", "response_preds"]
    },
    "metadata": {
      "variables": {
        "ROOT_PATH": "~/.deeppavlov",
        "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
        "MODELS_PATH": "{ROOT_PATH}/models",
        "SENTIMENT_MODELS_PATH": "{MODELS_PATH}/classifiers/rusentiment_v3",
        "INTENTS_MODELS_PATH": "{MODELS_PATH}/classifiers/deeppavlov_intents_cnn",
        "CONFIGS_PATH": "{DEEPPAVLOV_PATH}/configs"
      },
      "requirements": [
        "{DEEPPAVLOV_PATH}/requirements/tf.txt",
        "{DEEPPAVLOV_PATH}/requirements/gensim.txt",
        "{DEEPPAVLOV_PATH}/requirements/fasttext.txt"
      ],
      "labels": {
        "server_utils": "ChitChatRanker"
      },
      "download": [
        {
          "info": "1. Responses ranking model",
          "url": "http://files.deeppavlov.ai/deeppavlov_data/dev/chitchat_2staged_tfidf_smn_v16.tar.gz",
          "subdir": "{MODELS_PATH}"
        },
        {
          "info": "2. Sentiment classification model",
          "url": "http://files.deeppavlov.ai/deeppavlov_data/classifiers/rusentiment_v3.tar.gz",
          "subdir": "{ROOT_PATH}/models/classifiers"
        },
        {
          "info": "3. Intent classification model",
          "url": "http://files.deeppavlov.ai/deeppavlov_data/dev/deeppavlov_intents_cnn.tar.gz",
          "subdir": "{ROOT_PATH}/models/classifiers"
        },
        {
          "info": "FastText embeddings",
          "url": "http://files.deeppavlov.ai/embeddings/ft_native_300_ru_wiki_lenta_nltk_wordpunct_tokenize/ft_native_300_ru_wiki_lenta_nltk_wordpunct_tokenize.bin",
          "subdir": "{DOWNLOADS_PATH}/embeddings"
        }
      ]
    }
  }